package model;

public abstract class Product {
	//private Product(){};
	protected String make;
	protected int price;
	public abstract int calculatePrice();
	
	public synchronized int getBasePrice(){
		return this.price;
	}	
	public synchronized void setBasePriceINC(){
		this.price+=1;		
	}
	
	public synchronized void setBasePriceDEC(){
		this.price-=1;
		
	}
	public synchronized void setBasePrice(int price){
		this.price = price;
	}
	
}
