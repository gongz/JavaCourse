package model;

import java.util.ArrayList;

public class EditOptions {
	public EditOptions(){}
	
	//Edit automobile: model name
	public void EditModelName(String newName, Automobile targetAuto){
		targetAuto.setModelName(newName);
	}
	
	//Edit automobile: make
	public void EditMake(String newMake, Automobile targetAuto){
		targetAuto.setMake(newMake);
	}
	
	//Edit automobile: price
	public void EditBasePrice(int newPrice, Automobile targetAuto){
		targetAuto.setBasePrice(newPrice);
	}
	
	//Edit automobile: optionSetList
	public void EditOptionSetList(ArrayList<OptionSet> osl, Automobile targetAuto){
		targetAuto.setOptionSetList(osl);
	}
	
	//Edit OptionSet: name 
	public void EditOptionNameInOptionSet(String newName, OptionSet os, Automobile targetAuto){
		targetAuto.setOptionNameInOptionSet(os, newName);
	}
	
	//Edit OptionSet: optionsList
	public void EditOptionsInOptionSet(ArrayList<Option> ops, OptionSet os, Automobile targetAuto){
		targetAuto.setOptionsInOptionSet(os, ops);
	}
	
	//insert OptionSet: option
	public void AddOptionToOptionSet(){
		
	}
	
	
	
	
	
	
	
	
	
	
	

}
